<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="Eunsung.com">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/matzi5"
        android:label="로또맛집"
        android:roundIcon="@drawable/matzi5"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">
        <activity android:name=".lottoloadActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".lottoActivity"></activity>
        <activity android:name=".lottodtlActivity"></activity>
        <activity android:name=".mynumberActivity"></activity>
        <activity android:name=".lottohistActivity"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".numsaveActivity"
                    android:theme="@android:style/Theme.DeviceDefault.Light.Dialog"></activity>

        <activity
            android:name="com.google.android.gms.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"
            android:theme="@android:style/Theme.Translucent" />

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/application_id" />
        <meta-data

            android:name="com.google.android.gms.version"

            android:value="@integer/google_play_services_version" />
    </application>


</manifest>